        "cross-env": "^7.0",
        "dropzone": "^5.7.1",
        "es-jquery-sortable": "^0.9.13-patch2",
        "fontawesome-iconpicker": "^3.2.0",
        "jquery": "^3.2",
        "jquery-bonsai": "^2.1.3",
        "jquery-qubit": "^2.0.9",
        "laravel-mix": "^5.0.7",
