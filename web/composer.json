{
    "name": "aduxia/clavel-cms",
    "type": "project",
    "description": "Aduxia Clavel CMS",
    "keywords": [
        "Aduxia",
        "Clavel",
        "CMS",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "astrotomic/laravel-translatable": "^11.8",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "clavel/crud-generator": "@dev",
        "darkaonline/l5-swagger": "^7.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/agent": "^2.6",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.1",
        "nwidart/laravel-menus": "dev-add-laravel-7-support",
        "php-ffmpeg/php-ffmpeg": "^0.14.0",
        "phpoffice/phpspreadsheet": "^1.11",
        "proengsoft/laravel-jsvalidation": "^3.0",
        "santigarcor/laratrust": "^5.2",
        "webpatser/laravel-uuid": "^3.0",
        "yajra/laravel-datatables-oracle": "^9.9"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {
            "github.com": "432738027c9c541d6226ee7779e31958e2337ad5"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "test": "./vendor/bin/phpunit",
        "test-coverage": "./vendor/bin/phpunit --coverage-html coverage/"
    },
    "repositories": {
        "crud-generator": {
            "type": "path",
            "url": "./packages/clavel/crud-generator/"
        },
        "laravel-menus": {
            "type": "vcs",
            "url": "https://github.com/RocketLoom/laravel-menus"
        }
    }
}
