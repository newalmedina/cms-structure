{
  "name": "clavel/basic",
  "description": "Paquete de administración con menús front, páginas front y media files",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Juan Calvo",
      "email": "jjcalvo@aduxia.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2",
    "illuminate/support": "^5.6|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Clavel\\Basic\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Clavel\\Basic\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Clavel\\Basic\\BasicServiceProvider"
      ],
        "aliases": {
            "CustomMenu": "Clavel\\Basic\\Services\\CustomMenuFacade"
        }
    }
  }
}
