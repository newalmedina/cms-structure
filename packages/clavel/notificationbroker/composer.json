{
    "name": "clavel/notificationbroker",
    "description": "NotificationBroker SMS/Email/WhatsApp",
    "type": "library",
    "keywords": [
        "php",
        "Sms",
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Juan Calvo",
            "email": "jjcalvo@aduxia.com",
            "homepage": "http://www.aduxia.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0",
        "guzzlehttp/guzzle": "^6.3",
        "twilio/sdk": "^5.42",
        "php-imap/php-imap": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Clavel\\NotificationBroker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clavel\\NotificationBroker\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clavel\\NotificationBroker\\NotificationBrokerServiceProvider"
            ]
        }
    }
}
