{
  "name": "clavel/timetracker",
  "description": "Paquete de seguimiento de horas",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Juan Calvo",
      "email": "jjcalvo@aduxia.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2",
    "illuminate/support": "^5.6|^6.0|^7.0",
    "phpoffice/phpspreadsheet": "^1.8",
    "umpirsky/country-list": "^2.0",
    "umpirsky/currency-list": "^1.1"
  },
  "require-dev": {
