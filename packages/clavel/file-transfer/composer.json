{
  "name": "clavel/file-transfer",
  "description": "Paquete para el intercambio de ficheros",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Juan Calvo",
      "email": "jjcalvo@aduxia.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "illuminate/support": "5.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*|^6.3"
  },
  "autoload": {
    "psr-4": {
      "Clavel\\FileTransfer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Clavel\\FileTransfer\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Clavel\\FileTransfer\\FileTransferServiceProvider"
      ]
    }
  }
}
