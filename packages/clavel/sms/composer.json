{
  "name": "clavel/sms",
  "description": "Sms REST API library",
  "type": "library",
  "keywords": ["php","Sms","framework"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jose Juan Calvo",
      "email": "jjcalvo@aduxia.com",
      "homepage": "http://www.aduxia.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "illuminate/support": "5.7.*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*|^6.3"
  },
  "autoload": {
    "psr-4": {
      "Clavel\\Sms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Clavel\\Sms\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Clavel\\Sms\\SmsServiceProvider"
      ]
    }
  }
}
